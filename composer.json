{
  "name": "alabarentojr/random-user",
  "description": "Code challenge - Eonx",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "psr/http-client": "^1.0@dev",
    "symfony/http-client": "^5.2@dev",
    "symfony/console": "^5.2@dev",
    "illuminate/container": "^9.0@dev",
    "spatie/data-transfer-object": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "mockery/mockery": "^1.2"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Albert Labarento",
      "email": "albertlabarento@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\App\\": "tests/"
    }
  }
}
